*****************
counter = 0
print("Please enter some lines until you are finished.")
while True:
  lineIn = input()
  if lineIn == 'END':
    break
  counter = counter+1
  print('line', counter, '=', lineIn)

*****************
*****************
n=int(input())
counter = 1
while counter * counter < n:
   for counter in range(1, n):
      a = counter * counter
      if a >= n:
         break
      print(a)

*****************
*****************
counter = 0
while True:
  lineIn = input()
  if lineIn=='END':
    break
  if lineIn=='SKIP':
    continue
  counter = counter+1
  print('line', counter, '=', lineIn)  
  
