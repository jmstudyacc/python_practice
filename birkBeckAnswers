def isPalindrome(S):
   return S == S[::-1]

S = 'racecar'
ans = isPalindrome(S)

if ans:
   print("Yes")
else:
   print("No")
   
*****************
*****************
def prod(L):
   product = 1
   for i in L:
      product = product * i
   return product
   
****************
****************

 def mystery(x):
  a = [0, 4, 0, 3, 2]
  while x > 0:
    x = a[x]
  return "Done"
  
mystery(3)

